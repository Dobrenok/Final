<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//com.HibernateUtil/com.HibernateUtil Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="unt.UserEntity" table="User" schema="">
        <id name="id">
            <column name="id" sql-type="integer(0,-1)"/>
        </id>
        <property name="name">
            <column name="name" sql-type="text(0,-1)"/>
        </property>
        <property name="age">
            <column name="age" sql-type="integer(0,-1)"/>
        </property>
        <property name="isAdmin">
            <column name="isAdmin" sql-type="boolean(0,-1)"/>
        </property>
        <property name="createdData">
            <column name="createdData" sql-type="timestamp(0,-1)"/>
        </property>
    </class>
</hibernate-mapping>